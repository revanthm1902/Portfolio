{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/Resume.pdf",
      "dest": "/Resume.pdf",
      "headers": {
        "Content-Type": "application/pdf",
        "Content-Disposition": "inline; filename=\"Resume.pdf\""
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
